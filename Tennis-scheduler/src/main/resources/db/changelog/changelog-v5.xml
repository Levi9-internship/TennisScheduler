<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="nevena" id="drop-constraint-1">
        <dropUniqueConstraint constraintName="timeslot_person_id_key"
                              tableName="timeslot"
                              uniqueColumns="person_id"/>
    </changeSet>

    <changeSet author="nevena" id="drop-constraint-2">
        <dropUniqueConstraint constraintName="timeslot_tennis_court_id_key"
                              tableName="timeslot"
                              uniqueColumns="tennis_court_id"/>
    </changeSet>
</databaseChangeLog>


